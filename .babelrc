{
  "presets": [
    ["@babel/preset-env",
    {
      "targets": {
        "browsers": [
          ">0.25%",
          "not ie 11",
          "not op_mini all"
        ]
      }
    }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "react-hot-loader/babel"
  ],
  "env": {
    "production": {
      "presets": [
        "minify"
      ]
    }
  }
}
